#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin >> t;
    char ch[100001];
    int len, k = 1;
    while ( t-- )
    {
        int sum = 0;
        scanf ( "%s", &ch );
        len = strlen ( ch );
        printf ( "Case %d: ", k );
        for ( int i = 0 ; i < len ; i++ )
        {
            ch[i] = ch[i] - '0';
            sum += ch[i];
        }
        if ( ( sum % 2 == 0 && len % 2 == 0 ) || ( sum % 2 == 1 && len % 2 == 1 ) )
            cout << "Safe to push\n";
        else
            cout << "BOOM\n";
