#include<bits/stdc++.h>
using namespace std;
int main()
{
    freopen("c.txt","r",stdin);
    char ch[101];
    int len = strlen ( gets ( ch ) );
    //cout<<len;
    for ( int j = 0 ; j < len ; j++ )
    {
        if ( ch[j] >= 'a' &&  ch[j] <= 'z' )
        {
            ch[j] = ch[j]  32 ;
        }
    }
    cout << ch;
    return 0;
}



