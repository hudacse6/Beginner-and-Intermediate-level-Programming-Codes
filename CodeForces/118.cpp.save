#include<bits/stdc++.h>
using namespace std;
int main()
{
    char ar[101];
    gets(ar);
    for(int i=0 ; i< strlen(ar) ; i++)
    {
        if( ar[i]='A' || ar[i]='O' || ar[i]='Y' || ar[i]='E' || ar[i]='U'|| ar[i]='I' )
        {
            ar[i]=' ';
        }
        else
        {
            ar[i]=ar[i]-32;
            ar[i-1]='.';
        }
        cout<<ar;
    }
}
