#include<stdio.h>
#include<string.h>

int main()
{
    int Tcase, n, i;
    scanf ( "%d", &Tcase );
    char digit[100];
    int len;
    for ( i = 0; i < Tcase ; i++ )
    {
        scanf ( "%s", &digit );
        len = strlen ( digit );
        if ( digit ( len - 1 ) % 2 == 0 )
        {
            printf ( "even\n" );
        }
        else
        {
            printf ( "odd\n" );
        }
    }
    return 0;
}

