#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    int tcase;
    cin >> tcase;
    while ( tcase-- )
    {
        cin >> n;
        int m, i = 0, sum = 0;
        num2 = n;
        while ( n != 0 )
